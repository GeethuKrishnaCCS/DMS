{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "create-document-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/createDocument/CreateDocumentWebPart.js",
          "manifest": "./src/webparts/createDocument/CreateDocumentWebPart.manifest.json"
        }
      ]
    },
    "edit-document-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/editDocument/EditDocumentWebPart.js",
          "manifest": "./src/webparts/editDocument/EditDocumentWebPart.manifest.json"
        }
      ]
    },
    "send-request-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sendRequest/SendRequestWebPart.js",
          "manifest": "./src/webparts/sendRequest/SendRequestWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "CreateDocumentWebPartStrings": "lib/webparts/createDocument/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "EditDocumentWebPartStrings": "lib/webparts/editDocument/loc/{locale}.js",
    "SendRequestWebPartStrings": "lib/webparts/sendRequest/loc/{locale}.js"
  }
}
